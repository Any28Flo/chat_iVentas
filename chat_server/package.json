{
  "name": "chat_server",
  "version": "1.0.0",
  "description": "express server with pusher, graphQL in typescript",
  "main": "server.js",
  "scripts": {
    "start": "node  ./dist/server.js",
    "tsc": " npx tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/app.ts"
  },
  "author": "Erika Flores",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^15.8.0",
    "graphql-ws": "^5.14.0",
    "graphql-yoga": "^4.0.3",
    "pusher": "^5.1.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-graphql": "^0.9.0",
    "@types/node": "^20.4.4",
    "@types/ws": "^8.5.5",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
